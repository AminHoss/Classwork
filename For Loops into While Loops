#1
i = 0
my_str = "hello"
while i < len(my_str):
    char = my_str[i]
    print(char)
    i += 1

#2
i = 0
numbers = [7, 7, 2, 7, 11]
while i < len(numbers):
    num = numbers[i]
    print(num)
    i += 1

#3
i = 0
marks = [84, 89, 90, 45, 67]
while i < len(marks):
    mark = marks[i]
    print(mark)
    i += 1

#4
i = 0
while i < 10:
    print(i)
    i += 1

#5
i = 5
while i < 100:
    print(i)
    i += 5

#6
i = 100
while i > -1:
    print(i)
    i -= 5

